# Ignore Git-related files
.git
.gitignore
.gitmodules

# Ignore Python specific files and directories
__pycache__/
*.pyc
.pytest_cache/
.venv/
.env/
env/

# Ignore build output directories
build/
dist/
tmp/

# Ignore log files
*.log
logs/

